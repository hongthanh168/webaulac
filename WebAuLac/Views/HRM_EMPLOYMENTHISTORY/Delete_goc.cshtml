@model WebAuLac.Models.HRM_EMPLOYMENTHISTORY

@{
    string hoten = Model.HRM_EMPLOYEE.FirstName + " " + Model.HRM_EMPLOYEE.LastName;
}
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">Bạn có chắc muốn xóa bằng cấp này?</h4>
</div>
<div class="modal-body">

    <table class="table no-border table-striped table-hover">
        <tr>
            <th>Số QĐ:</th>

            <td>@Html.DisplayFor(model => model.DecisionNo)</td>
        </tr>
        <tr>
            <th>@Html.DisplayNameFor(model => model.DecisionDate):
            </th>

            <td>
                @Html.DisplayFor(model => model.DecisionDate)
            </td>
        </tr>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EffectiveDate):
            </th>

            <td>
                @Html.DisplayFor(model => model.EffectiveDate)
            </td>
        </tr>


        <tr>
            <th>
             Loại quyết định:
            </th>

            <td>
                @Html.DisplayFor(model => model.DIC_CATEGORYDECISION.CategoryDecisionName)
            </td>
        </tr>      

        
    </table>
</div>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="modal-footer">
        <button class="btn btn-default" data-dismiss="modal" style="margin-bottom:0px;">
            <i class="fa fa-ban"></i> Hủy
        </button>
        <button class="btn btn-danger" type="submit">
            <i class="fa fa-trash"></i>Xóa
        </button>
    </div>
}
