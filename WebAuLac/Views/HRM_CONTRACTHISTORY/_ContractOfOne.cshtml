@model IEnumerable< WebAuLac.Models.HRM_CONTRACTHISTORY>

<p>
    <button class="btn btn-primary" onclick="HienThiModal('@Url.Action("CreateOne", "HRM_CONTRACTHISTORY" ,new { EmployeeID=ViewBag.EmployeeID})', 'tab_content5' )"><i class="fa fa-plus m-right-xs"></i>Thêm mới</button>
 </p>
<table id="ContractHistoryList" class="table table-bordered action-table">
    <thead>
        <tr>
            <th>STT</th>
            <th>
                @Html.DisplayNameFor(model => model.ContractNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContractDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EffctiveDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExpirationDate)
            </th>
           
            <th>
                @Html.DisplayNameFor(model => model.ContractTypeID)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @{
        int stt =0;
        }
        @foreach (var item in Model)
        {
        stt = stt + 1;
            string rowID = "HRM_CONTRACTHISTORY_" + item.ContractHistoryID.ToString();
        <tr id="@rowID">
            <td>@stt</td>
            <td>
                @Html.DisplayFor(modelItem => item.ContractNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContractDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EffctiveDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExpirationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DIC_CONTRACTTYPE.ContractTypeName)
            </td>
          
            <td>
                <a href="#" onclick="HienThiModal('@Url.Action("Edit", "HRM_CONTRACTHISTORY" ,new {id=item.ContractHistoryID})', 'tab_content5' )" class="btn btn-success btn-xs"><i class="fa fa-edit"></i></a>
                <a href="#" onclick="HienThiModal('@Url.Action("Delete", "HRM_CONTRACTHISTORY" ,new {id=item.ContractHistoryID})', 'tab_content5' )" class="btn btn-danger btn-xs"><i class="fa fa-trash"></i></a>
            </td>
        </tr>
        }
    </tbody>
</table>



