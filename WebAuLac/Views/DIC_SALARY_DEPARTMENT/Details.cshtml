@model WebAuLac.Models.DIC_DEGREE

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">Chi tiết bằng cấp</h4>
</div>
<div class="modal-body">
    <table class="table no-border table-striped table-hover">
        <tr>
            <th>@Html.DisplayNameFor(model => model.DegreeName):</th>
            <td>@Html.DisplayFor(model => model.DegreeName)</td>
        </tr>  
        <tr>
            <th>@Html.DisplayNameFor(model => model.Description):</th>
            <td>@Html.DisplayFor(model => model.Description)</td>
        </tr>             
    </table>
</div>
<div class="modal-footer">
    <button class="btn btn-success" data-dismiss="modal">OK</button>
</div>
