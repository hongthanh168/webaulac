@model WebAuLac.Models.tbl_ctdaotao
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">Bạn có chắc muốn xóa chi tiết đào tạo này?</h4>
</div>

<div class="modal-body">
    <table class="table no-border table-striped table-hover">
        <tr>
            <th>@Html.DisplayNameFor(model => model.tbl_khoadaotao.tenkhoadaotao):</th>
            <td>@Html.DisplayFor(model => model.tbl_khoadaotao.tenkhoadaotao)</td>
        </tr>
        <tr>
            <th>Họ tên:</th>
            <td>@Html.DisplayFor(model => model.HRM_EMPLOYEE.HoTen)</td>
        </tr>
        <tr>
            <th>Chức vụ:</th>
            <td>@Html.DisplayFor(model => model.ChucVu)</td>
        </tr>
        <tr>
            <th>@Html.DisplayNameFor(model => model.ketqua):</th>
            <td>@Html.DisplayFor(model => model.ketqua)</td>
        </tr>
    </table>
</div>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="modal-footer">
        <button class="btn btn-default" data-dismiss="modal" style="margin-bottom:0px;">
            <i class="fa fa-ban"></i> Hủy
        </button>
        <button class="btn btn-danger" type="submit">
            <i class="fa fa-trash"></i>Xóa
        </button>
    </div>
}