@model WebAuLac.Models.tbl_ctdaotao

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">Chi tiết đào tạo</h4>
</div>
<div class="modal-body">
    <table class="table no-border table-striped table-hover">
        <tr>
            <th>@Html.DisplayNameFor(model => model.tbl_khoadaotao.tenkhoadaotao):</th>
            <td>@Html.DisplayFor(model => model.tbl_khoadaotao.tenkhoadaotao)</td>
        </tr>
        <tr>
            <th>Họ tên:</th>
            <td>@Html.DisplayFor(model => model.HRM_EMPLOYEE.HoTen)</td>
        </tr>
        <tr>
            <th>@Html.DisplayNameFor(model => model.ketqua):</th>
            <td>@Html.DisplayFor(model => model.ketqua)</td>
        </tr>
        <tr>
            <th>@Html.DisplayNameFor(model => model.GhiChu):</th>
            <td>@Html.DisplayFor(model => model.GhiChu)</td>
        </tr>
    </table>
</div>
<div class="modal-footer">
    <button class="btn btn-success" data-dismiss="modal">OK</button>
</div>